{
  "name": "n8n-nodes-starter",
  "version": "0.1.0",
  "description": "A starter template for creating n8n community nodes",
  "keywords": [
    "n8n-community-node-package",
    "starter template"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Miquel Colomer",
    "email": "contact@n8nhackers.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/n8nhackers/n8n-nodes-starter.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "dev:n8n": "nodemon",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest --runInBand"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExampleCredentialsApi.credentials.js",
      "dist/credentials/HttpBinApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/action/N8nHackersHelloWorld/N8nHackersHelloWorld.node.js",
      "dist/nodes/action/N8nHackersHelloWorld2/N8nHackersHelloWorld2.node.js",
      "dist/nodes/action/N8nHackersHelloWorld3/N8nHackersHelloWorld3.node.js",
      "dist/nodes/action/N8nHackersHttpBin/N8nHackersHttpBin.node.js",
      "dist/nodes/trigger/N8nHackersSecondsTrigger/N8nHackersSecondsTrigger.node.js",
      "dist/nodes/trigger/N8nHackersWebhookTrigger/N8nHackersWebhookTrigger.node.js",
      "dist/nodes/trigger/N8nHackersN8nTrigger/N8nHackersN8nTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
